<idea-plugin>
    <id>Java Builder Generator</id>
    <name>Java Builder Generator</name>
    <version>1.0</version>

    <description><![CDATA[
     Java Builder Generator
    ]]></description>

    <change-notes><![CDATA[
      07.06.2018 - 1.0 - First release <br/>
    ]]>
    </change-notes>

    <idea-version since-build="173.0"/>

    <extensions defaultExtensionNs="com.intellij">
    </extensions>


    <actions>
        <action id="org.idea.plugin.builder" class="org.idea.plugin.jbgenerator.BuilderCodeInsightAction" text="Java Builder Generator"
                description="Java Builder Generator">
            <add-to-group group-id="JavaGenerateGroup1" anchor="first"/>
            <keyboard-shortcut keymap="$default" first-keystroke="shift alt b"/>
        </action>
    </actions>
</idea-plugin>